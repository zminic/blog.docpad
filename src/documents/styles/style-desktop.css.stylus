@media screen and (min-width: 961px)
	body
	.header
		.social-icons
			display: block;
			position: absolute
			top: 0
			right: 5px
			width: 60px
			height: 26px
			border-radius: 6px
			text-align: center
			padding-top: 5px
	.siteContainer
		width: 960px
		margin: 0 auto
	.menu
		li
			a
				&:hover
					color: #666
